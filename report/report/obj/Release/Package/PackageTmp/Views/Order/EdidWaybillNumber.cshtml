@model report.Models.OrderProm

@using (Ajax.BeginForm("EdidWaybillNumber", "Order", new AjaxOptions { UpdateTargetId = "Save_changes_" + Model.Id , InsertionMode=InsertionMode.Replace }))
{
    @Html.AntiForgeryToken()

    @Html.HiddenFor(model => model.Id)
    @Html.HiddenFor(model => model.Id_User)
    @Html.HiddenFor(model => model.number)
    @Html.EditorFor(model => model.WaybillNumber, new { htmlAttributes = new { @class = "form-control", data_typeahead = Model.WaybillNumber } })
    <input type="submit" value="Save" class="btn btn-default" />
}
